{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true,
      "mocha": true
  },
  "rules": {
      "indent": [2, 2],
      "comma-dangle": 0
  },
  "globals": {
    "initialState": true,
    "describe": true,
    "it": true,
    "expect": true,
    "__DEV__": true
  }
}
